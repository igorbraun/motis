include "paxmon/CompactJourney.fbs";

namespace motis.paxmon;

table DataSource {
  primary_ref: ulong;
  secondary_ref: ulong;
}

table PassengerGroup {
  id: ulong;
  source: DataSource;
  passenger_count: uint;
  planned_journey: CompactJourney;
  probability: float;
  planned_arrival_time: ulong;
  source_flags: ubyte;
}
